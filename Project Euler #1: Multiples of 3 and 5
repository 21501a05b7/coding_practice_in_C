#include<stdio.h>
#include<math.h>
int main()
{
    long int n,i,a,b,c,t;
    scanf("%ld",&t);            
    for(i=0;i<t;i++){          
        scanf("%ld",&n);       
        a = (n-1)/3;           
        b = (n-1)/5;
        c = (n-1)/15;
        printf("%ld\n",3*(a*(a+1)/2) + 5*(b*(b+1)/2) - 15*(c*(c+1)/2));
    }
    return 0;
}
